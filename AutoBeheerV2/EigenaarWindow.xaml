<Window x:Class="AutoBeheerV2.EigenaarWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoBeheerV2"
        mc:Ignorable="d"
        Title="EigenaarWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtNaam" Text="{Binding Naam}"  HorizontalAlignment="Left" Height="23" Margin="203,86,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnOpslaan" Content="Opslaan" HorizontalAlignment="Left" Margin="203,140,0,0" VerticalAlignment="Top" Width="75" Click="btnOpslaan_Click"/>
        <Button x:Name="btnSluiten" Content="Sluiten" HorizontalAlignment="Left" Margin="375,140,0,0" VerticalAlignment="Top" Width="75" Click="btnSluiten_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="69,86,0,0" TextWrapping="Wrap" Text="Naam:" VerticalAlignment="Top" Height="23" Width="120"/>

    </Grid>
</Window>
